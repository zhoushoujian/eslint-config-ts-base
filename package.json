{
  "name": "eslint-config-ts-base",
  "description": "eslint config for typescript&react",
  "license": "MIT",
  "version": "1.1.1",
  "main": "index.js",
  "homepage": "https://github.com/zhoushoujian/ESLint",
  "scripts": {
    "checkcode:all": "npm run checkcode:index && npm run checkcode:base && npm run checkcode:react && npm run checkcode:vue && npm run checkcode:typescript",
    "checkcode:index": "eslint -c ./test/.eslintrc.index.js \"./test/index.ts\"",
    "checkcode:base": "eslint -c ./test/.eslintrc.base.js \"./test/base.js\"",
    "checkcode:react": "npx eslint -c ./test/.eslintrc.react.js \"./test/react.jsx\"",
    "checkcode:vue": "eslint -c ./test/.eslintrc.vue.js \"./test/vue.vue\"",
    "checkcode:typescript": "eslint -c ./test/.eslintrc.ts.js \"./test/typescript.ts\"",
    "publishToNpm": "node scripts/publish",
    "postpublishToNpm": "git push && git push origin --tags"
  },
  "files": [
    "rules",
    "test",
    ".prettierrc.js",
    "base.js",
    "CHANGELOG.md",
    "comment.js",
    "index.js",
    "package.json",
    "prettier.js",
    "react.js",
    "README.md",
    "tsconfig.json",
    "typescript.js",
    "vue.js"
  ],
  "dependencies": {
    "@shuyun-ep-team/specified-package-version-check": "^1.10.6",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.15.1",
    "figlet": "^1.6.0",
    "prettier": "^2.8.8",
    "vue-eslint-parser": "^9.3.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "config",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide",
    "typescript"
  ]
}
